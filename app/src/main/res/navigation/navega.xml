<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navega"
    app:startDestination="@id/listaFragment2">

    <fragment
        android:id="@+id/listaFragment2"
        android:name="app.agenda.agendanotas.ui.view.fragment.ListaFragment"
        android:label="Lista de Recordatorios"
        tools:layout="@layout/fragment_lista" >
        <action
            android:id="@+id/action_listaFragment2_to_crearFragment2"
            app:destination="@id/crearFragment2" />
        <action
            android:id="@+id/action_listaFragment2_to_editarFragment"
            app:destination="@id/editarFragment" />
    </fragment>
    <fragment
        android:id="@+id/crearFragment2"
        android:name="app.agenda.agendanotas.ui.view.fragment.CrearFragment"
        android:label="Crear Recordatorio"
        tools:layout="@layout/fragment_crear" />
    <fragment
        android:id="@+id/editarFragment"
        android:name="app.agenda.agendanotas.ui.view.fragment.EditarFragment"
        android:label="Editar Recordatorio"
        tools:layout="@layout/fragment_editar" />
</navigation>